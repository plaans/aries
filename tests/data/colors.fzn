% Coloring problem in Minizinc
%
%% From 
%% Katta G. Murty: "Optimization Models for Decision Making", page 344f
%% http://ioe.engin.umich.edu/people/fac/books/murty/opti_model/junior-7.pdf
%
% Model created by Hakan Kjellerstrand, hakank@bonetmail.com
% See also my MiniZinc page: http://www.hakank.org/minizinc

array [1..2] of int: X_INTRODUCED_7_ = [1,-1];
var 0..6: numColors:: output_var;
var 1..6: X_INTRODUCED_0_;
var 1..6: X_INTRODUCED_1_;
var 1..6: X_INTRODUCED_2_;
var 1..6: X_INTRODUCED_3_;
var 1..6: X_INTRODUCED_4_;
var 1..6: X_INTRODUCED_5_;
array [1..6] of var int: x:: output_array([1..6]) = [X_INTRODUCED_0_,X_INTRODUCED_1_,X_INTRODUCED_2_,X_INTRODUCED_3_,X_INTRODUCED_4_,X_INTRODUCED_5_];
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_2_,X_INTRODUCED_0_],0); % x2 <= x0
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_2_,X_INTRODUCED_5_],0); % x2 <= x5
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_2_,X_INTRODUCED_3_],0); % x2 <= x3
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_5_,X_INTRODUCED_3_],0); % x5 <= x3
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_5_,X_INTRODUCED_0_],0); % x5 <= x0
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_0_,X_INTRODUCED_4_],0); % x0 <= x4
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_0_,X_INTRODUCED_3_],0); % x0 <= x3
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_3_,X_INTRODUCED_4_],0); % x3 <= x4
constraint int_lin_ne(X_INTRODUCED_7_,[X_INTRODUCED_3_,X_INTRODUCED_1_],0); % x3 <= x1
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_0_,numColors],0); % x0 <= numColors
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_1_,numColors],0); % x1 <= numColors
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_2_,numColors],0); % x2 <= numColors
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_3_,numColors],0); % x3 <= numColors
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_4_,numColors],0); % x4 <= numColors
constraint int_lin_le(X_INTRODUCED_7_,[X_INTRODUCED_5_,numColors],0); % x5 <= numColors
solve  minimize numColors;